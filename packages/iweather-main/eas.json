{
	"cli": {
		"version": ">= 10.0.2"
	},
	"build": {
		"preview": {
			"autoIncrement": true,
			"android": {
				"distribution": "internal",
				"autoIncrement": "versionCode"
			},
			"ios": {
				"simulator": true
			}
		},
		"production": {
			"autoIncrement": true,
			"android": {
				"autoIncrement": "versionCode"
			}
		}
	},
	"submit": {
		"preview": {
			"android": {
				"track": "internal",
				"servicesAccountKeyPath": "google-services.json",
				"releaseStatus": "draft"
			}
		}
	}
}
